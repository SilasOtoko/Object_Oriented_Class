package warpCore;

public class WarpCore {

	public static void main(String[] args) {
		Input input = new Input();
		Dice dice = new Dice();
		int turns = 0;
		int d1, d2, d3;
		boolean r1, r2, r3;
		r1 = r2 = r3 = true;
		
		while(turns < 3){
			//roll the three dice
			d1 = dice.roll();
			d2 = dice.roll();
			d3 = dice.roll();
			System.out.println(d1 + " " + d2 + " " + d3);
			turns++;
		}
	}
}
